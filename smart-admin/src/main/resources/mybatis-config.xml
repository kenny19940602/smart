<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org/DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

    <settings>
        <setting name="aggressiveLazyLoading" value="false"/>
    </settings>
    <typeAliases>
        <package name="com.kenny.smart.model"/>
    </typeAliases>
    <typeHandlers>
        <typeHandler handler="com.kenny.smart.type.handler.UserStatusTypeHandler"
                     javaType="com.kenny.smart.type.UserStatus"/>
    </typeHandlers>
</configuration>

